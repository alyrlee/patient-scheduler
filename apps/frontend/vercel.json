{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://backend-phi-seven-76.vercel.app/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://backend-phi-seven-76.vercel.app/health"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
